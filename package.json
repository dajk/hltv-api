{
  "name": "hltv-api",
  "version": "3.0.3",
  "description": "API from Hltv.org",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "workspaces": [
    "examples/*",
    "website"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "cover": "jest --coverage",
    "lint": "eslint --ext js,ts,tsx ./src",
    "format": "prettier --write '**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest",
    "test:w": "yarn test --watch",
    "prebuild": "rimraf dist",
    "build": "tsc -p .",
    "build:w": "tsc -p . -w",
    "tsc": "tsc",
    "prepare": "husky install"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dajk/hltv-api.git"
  },
  "keywords": [
    "hltv.org",
    "api",
    "cs:go",
    "csgo",
    "Counter-Strike: Global Offensive",
    "node",
    "node.js",
    "nodejs"
  ],
  "author": {
    "email": "me@radovanhajdukovic.com",
    "name": "Radovan Hajdukovic",
    "url": "https://www.radovanhajdukovic.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dajk/hltv-api/issues"
  },
  "homepage": "https://github.com/dajk/hltv-api#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^2.6.0",
    "user-agents": "^1.0.1195",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "@types/user-agents": "^1.0.2",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "codecov": "^3.8.3",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix"
  }
}
